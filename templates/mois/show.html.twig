{% extends 'baseAdmin.html.twig' %}
{% block title %}Mois{% endblock %}

{% block body %}
    <h1>Mois</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ mois.id }}</td>
            </tr>
            <tr>
                <th>Mois</th>
                <td>{{ mois.mois }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('mois_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('mois_edit', { 'id': mois.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
